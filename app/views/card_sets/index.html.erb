<div class="row" id="accordion">
	<% @series.each do |series| %>
	<div class="col-xs-12">
		<a data-toggle="collapse" data-parent="#accordion" href=<%= "#collapse#{series.id}" %>>
    	<%= image_tag(image_url(series.logo_image_url),  style:"height: 150px; max-width: 250px;") %> <%=  series.name %>
  	</a>
			<div class="row panel-collapse collapse" id=<%= "collapse#{series.id}" %>>
				<% series.card_sets.each do |card_set| %>
					<div class="col-xs-12 col-sm-3">
						<h2><%= card_set.name %></h2>
						<div><%= image_tag(image_url(card_set.logo_image_url),  style:"height: 150px; max-width: 250px;") %></div>
					</div>
				<% end %>
			</div>
		</div>
	<% end %>
</div>